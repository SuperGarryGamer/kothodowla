<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.MainController">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#dadada" height="10.0" layoutY="95.0" stroke="#dadada" strokeType="INSIDE" width="642.0" />
      <HBox alignment="CENTER" layoutX="-2.0" prefHeight="100.0" prefWidth="642.0" spacing="30.0">
         <children>
            <TextField fx:id="name" prefHeight="25.0" prefWidth="143.0" promptText="Name" />
            <ComboBox fx:id="mother" prefHeight="25.0" prefWidth="99.0" promptText="Mother" />
            <ComboBox fx:id="father" prefHeight="25.0" prefWidth="99.0" promptText="Father" />
            <CheckBox fx:id="isFemale" mnemonicParsing="false" onAction="#isFemale" text="Female" />
            <Button mnemonicParsing="false" onAction="#add" prefHeight="42.0" prefWidth="73.0" text="Add" />
         </children>
      </HBox>
      <AnchorPane fx:id="familyTree" layoutY="105.0" prefHeight="495.0" prefWidth="642.0" />
   </children>
</AnchorPane>
